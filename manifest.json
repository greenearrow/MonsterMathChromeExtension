{
  "name": "DND Beyond Go!",
  "description": "Capture DND Beyond stat blocks when you load the page",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_title": "Monster Math"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "options_page": "options.html",
  "permissions": ["tabs","downloads","storage"]
}